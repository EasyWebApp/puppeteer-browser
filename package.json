{
  "name": "puppeteer-browser",
  "version": "0.2.1",
  "description": "Multiple Web browser controller based on Puppeteer API",
  "main": "dist/index.js",
  "engines": {
    "node": "^6.5.0"
  },
  "scripts": {
    "build": "babel source/ -d dist/  &&  esdoc",
    "start": "web-server docs/ -o",
    "precommit": "eslint source/",
    "prepublishOnly": "npm run build",
    "preinstall": "npm set puppeteer_skip_chromium_download true  &&  app-find chrome firefox -c",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/EasyWebApp/puppeteer-browser.git"
  },
  "keywords": [
    "Web",
    "browser",
    "Puppeteer",
    "testing",
    "automation"
  ],
  "author": "shiy2008@gmail.com",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/EasyWebApp/puppeteer-browser/issues"
  },
  "homepage": "https://easywebapp.github.io/puppeteer-browser",
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "babel-runtime": "^6.26.0",
    "chokidar": "^2.0.3",
    "fs-match": "^1.3.1",
    "koapache": "^0.2.3",
    "puppeteer": "^1.5.0",
    "puppeteer-fx": "0.0.4"
  },
  "optionalDependencies": {
    "puppeteer-ie": "^0.5.4"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.2.3",
    "babel-plugin-dynamic-import-node": "^1.2.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.7.0",
    "esdoc": "^1.1.0",
    "esdoc-ecmascript-proposal-plugin": "^1.0.0",
    "esdoc-external-nodejs-plugin": "^1.0.0",
    "esdoc-standard-plugin": "^1.0.0",
    "eslint": "^4.19.1",
    "husky": "^0.14.3"
  },
  "babel": {
    "presets": [
      "env"
    ],
    "plugins": [
      "transform-runtime",
      "dynamic-import-node"
    ]
  }
}
